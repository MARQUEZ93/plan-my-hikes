{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "aemarquez1993/plan-my-hikes-backend",
        "hostname": "backend",
        "essential": false,
        "memory": 128
      },
      {
        "name": "client",
        "image": "aemarquez1993/plan-my-hikes-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "aemarquez1993/nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "backend"],
        "memory": 128
      }
    ]
  }